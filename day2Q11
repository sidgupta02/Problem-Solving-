
#include <stdio.h>

int main()
{
    
    for(int i=1;i<=5;i++){
        for(int j=1;j<=5;j++){
            if(j<=i){
                printf("%d ",j);
            }
            else
                printf("  ");
        }
        for(int k=5;k>0;k--){
            if(k<=i){
                printf("%d ",k);
            }
            else
                printf("  ");
                
        }
        printf("\n");
    }

    return 0;
}
